<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.hcl.CFIN-Report.controller.ReportDetails" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="CFIN Update Assessment Analysis Report" titleLevel="H2" class="sapUiContentPadding customPage" showNavButton="false">
		<headerContent>
			<Image src="mime/Fonterra.png" height="40px"/>
		</headerContent>
		<content>
			<IconTabBar id="iconTabBarDetails" expanded="true" expandable="false" select="onSelectLevelOneTab" class="sapUiResponsiveContentPadding customFilterWrap customFilterText" headerBackgroundDesign="Transparent" backgroundDesign="Translucent">
				<items>
					<IconTabFilter id="iconTabOverview" icon="sap-icon://overview-chart" text="Overview" key="Overview"/>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter id="iconTabESummary" icon="sap-icon://along-stacked-chart" text="Executive Summary" key="ExSummary"/>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter id="iconTabSourceSystem" icon="sap-icon://it-system" text="Source System" key="SourceSystem"/>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter id="iconTabSLT" icon="sap-icon://database" text="SLT" key="SLT"/>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter id="iconTabTargetSystem" icon="sap-icon://goal" text="Target System" key="TargetSystem"/>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter id="iconTabAIF" icon="sap-icon://it-system" text="AIF" key="AIF"/>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter id="iconTabMDG" icon="sap-icon://master-task-triangle-2" text="MDG" key="MDG"/>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter id="iconTabNonSAP" icon="sap-icon://outbox" text="Non-SAP / External Interface" key="NonSAP"/>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter id="iconTabWhatsNext" icon="sap-icon://sys-find-next" text="What's Next" key="WhatsNext"/>
				</items>
			</IconTabBar>
		</content>
		<footer>
			<OverflowToolbar>
				<Button id="btnPrevious" type="Accept" icon="sap-icon://close-command-field" tooltip="Prevoius" press="onPressPreviousTab" visible="false"/>
				<ToolbarSpacer/>
				<HBox>
					<Title level="H5" text="Report generated by" class="footerText"/>
					<Image src="mime/HCL.png" width="75px" class="footerLogo"/>
				</HBox>
				<ToolbarSpacer/>
				<Button id="btnNext" type="Accept" icon="sap-icon://open-command-field" tooltip="Next" press="onPressNextTab"/>
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>