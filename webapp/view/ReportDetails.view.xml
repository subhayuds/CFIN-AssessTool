<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.hcl.CFIN-Report.controller.ReportDetails" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="CFIN Update Assessment Analysis Report" titleLevel="H2" class="sapUiContentPadding customPage" showNavButton="false">
		<headerContent>
			<Image src="mime/Fonterra.png" height="40px"/>
		</headerContent>
		<content>
			<IconTabBar id="iconTabBarDetails" expanded="{device>/isNoPhone}" select="onSelectLevelOneTab" class="sapUiResponsiveContentPadding customFilterWrap customFilterText" headerBackgroundDesign="Transparent" backgroundDesign="Translucent">
				<items>
					<IconTabFilter id="iconTabOverview" icon="sap-icon://overview-chart" text="Overview" key="overview"/>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter icon="sap-icon://along-stacked-chart" text="Executive Summary" key="exSummary">
						<Text text="Executive Summary content go here ..." />
					</IconTabFilter>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter icon="sap-icon://it-system" text="Source System" key="sourceSystem">
						<Text text="Source System content go here ..." />
					</IconTabFilter>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter icon="sap-icon://database" text="SLT" key="slt">
						<Text text="SLT content goes here ..." />
					</IconTabFilter>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter icon="sap-icon://goal" text="Target System" key="targetSystem">
						<Text text="Target System content goes here ..." />
					</IconTabFilter>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter icon="sap-icon://it-system" text="AIF" key="aif">
						<Text text="Target System content goes here ..." />
					</IconTabFilter>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter icon="sap-icon://master-task-triangle-2" text="MDG" key="mdg">
						<Text text="Target System content goes here ..." />
					</IconTabFilter>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter icon="sap-icon://outbox" text="Non-SAP / External Interface" key="external">
						<Text text="Target System content goes here ..." />
					</IconTabFilter>
					<IconTabSeparator icon="sap-icon://step" />
					<IconTabFilter icon="sap-icon://sys-find-next" text="What's Next" key="whatNext">
						<Text text="Target System content goes here ..." />
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
		<footer>
			<OverflowToolbar>
				<Button type="Accept" icon="sap-icon://close-command-field" tooltip="Prevoius"/>
				<ToolbarSpacer/>
				<HBox>
					<Title level="H5" text="Report generated by" class="footerText"/>
					<Image src="mime/HCL.png" width="75px" class="footerLogo"/>
				</HBox>
				<ToolbarSpacer/>
				<Button type="Accept" icon="sap-icon://open-command-field" tooltip="Next"/>
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>